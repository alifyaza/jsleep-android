<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2196F3"
    android:visibility="visible"
    tools:context="AboutMeActivity">

    <Button
        android:id="@+id/topup_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="88dp"
        android:layout_weight="1"
        android:backgroundTint="#FF9800"
        android:text="TOP UP"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/displaybalance" />

    <EditText
        android:id="@+id/Amount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:layout_marginTop="44dp"
        android:ems="7"
        android:gravity="center"
        android:inputType="number"
        android:minHeight="48dp"
        android:text="Amount"
        android:textColor="#FFFFFF"
        android:textSize="13sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outputbalance" />

    <TextView
        android:id="@+id/outputbalance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="24dp"
        android:gravity="left"
        android:letterSpacing="0.06"
        android:lineSpacingExtra="-6sp"
        android:text="Balance"
        android:textColor="#FFFFFF"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outputemail" />

    <TextView
        android:id="@+id/outputemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="26dp"
        android:gravity="left"
        android:letterSpacing="0.06"
        android:lineSpacingExtra="-6sp"
        android:text="Email"
        android:textColor="#FFFFFF"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/outputname" />

    <TextView
        android:id="@+id/outputname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="34dp"
        android:gravity="left"
        android:letterSpacing="0.06"
        android:lineSpacingExtra="-6sp"
        android:text="Name"
        android:textColor="#FFFFFF"
        android:textSize="13sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/AccountDetails" />

    <TextView
        android:id="@+id/displaybalance"
        android:layout_width="134dp"
        android:layout_height="15dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="46dp"
        android:gravity="end"
        android:letterSpacing="0.06"
        android:lineSpacingExtra="-6sp"
        android:text="0.0"
        android:textColor="#FFFFFF"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/displayemail" />

    <TextView
        android:id="@+id/displayname"
        android:layout_width="132dp"
        android:layout_height="20dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="114dp"
        android:layout_marginEnd="46dp"
        android:gravity="end"
        android:letterSpacing="0.06"
        android:lineSpacingExtra="-6sp"
        android:text="Alifya"
        android:textColor="#FFFFFF"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/displayemail"
        android:layout_width="132dp"
        android:layout_height="18dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="26dp"
        android:layout_marginEnd="46dp"
        android:gravity="end"
        android:letterSpacing="0.06"
        android:lineSpacingExtra="-6sp"
        android:text="alifya@ui.ac.id"
        android:textColor="#FFFFFF"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/displayname" />

    <TextView
        android:id="@+id/AccountDetails"
        android:layout_width="184dp"
        android:layout_height="0dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="46dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="180dp"
        android:gravity="top"
        android:letterSpacing="0.06"
        android:lineSpacingExtra="-6sp"
        android:text="Account Details"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/RegisterRenterButton"
        android:layout_width="314dp"
        android:layout_height="48dp"
        android:layout_marginStart="46dp"
        android:layout_marginEnd="46dp"
        android:backgroundTint="#FE9700"
        android:text="Register Renter"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.472" />

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewRegister"
        android:layout_width="360dp"
        android:layout_height="290dp"
        android:layout_marginBottom="45dp"
        app:cardBackgroundColor="#2196F3"
        app:cardCornerRadius="30dp"
        app:cardElevation="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layout"
            android:layout_width="match_parent"
            android:layout_height="344dp"
            android:backgroundTint="#2196F3">

            <EditText
                android:id="@+id/RenterName"
                android:layout_width="285dp"
                android:layout_height="45dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="35dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="35dp"
                android:drawablePadding="25dp"
                android:hint="Name"
                android:inputType="text"
                android:minHeight="48dp"
                android:padding="10dp"
                android:textColor="#000000"
                android:textColorHint="#FFFFFF"
                android:textSize="13sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="TouchTargetSizeCheck" />

            <EditText
                android:id="@+id/RenterAddress"
                android:layout_width="285dp"
                android:layout_height="45dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="35dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="35dp"
                android:drawablePadding="25dp"
                android:hint="Address"
                android:inputType="text"
                android:minHeight="48dp"
                android:padding="10dp"
                android:textColor="#000000"
                android:textColorHint="#FFFFFF"
                android:textSize="13sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/RenterName"
                tools:ignore="TouchTargetSizeCheck" />

            <EditText
                android:id="@+id/RenterPhoneNumber"
                android:layout_width="285dp"
                android:layout_height="46dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="35dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="35dp"
                android:drawablePadding="25dp"
                android:hint="Phone Number"
                android:inputType="text"
                android:minHeight="48dp"
                android:padding="10dp"
                android:textColor="#000000"
                android:textColorHint="#FFFFFF"
                android:textSize="13sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/RenterAddress"
                tools:ignore="TouchTargetSizeCheck" />

            <Button
                android:id="@+id/RegisterReq"
                android:layout_width="131dp"
                android:layout_height="52dp"
                android:layout_marginStart="35dp"
                android:layout_marginTop="25dp"
                android:layout_marginBottom="75dp"
                android:backgroundTint="#FE9700"
                android:text="REGISTER"
                android:textColor="#FFFFFF"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/RenterPhoneNumber"
                tools:ignore="TouchTargetSizeCheck" />

            <Button
                android:id="@+id/CancelReq"
                android:layout_width="135dp"
                android:layout_height="50dp"
                android:layout_marginTop="25dp"
                android:layout_marginEnd="35dp"
                android:backgroundTint="#FE9700"
                android:text="CANCEL"
                android:textColor="#FFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/RenterPhoneNumber"
                tools:ignore="TouchTargetSizeCheck" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardViewInput"
        android:layout_width="360dp"
        android:layout_height="290dp"
        android:layout_marginBottom="45dp"
        app:cardBackgroundColor="#FF9800"
        app:cardCornerRadius="30dp"
        app:cardElevation="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="344dp"
            android:backgroundTint="#FF9800">

            <TextView
                android:id="@+id/YourData"
                android:layout_width="184dp"
                android:layout_height="0dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="88dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="88dp"
                android:gravity="center|top"
                android:letterSpacing="0.06"
                android:lineSpacingExtra="-6sp"
                android:text="Your Data"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/PhoneNumberOut"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="40dp"
                android:layout_marginBottom="110dp"
                android:gravity="left"
                android:letterSpacing="0.06"
                android:lineSpacingExtra="-6sp"
                android:text="Phone Number"
                android:textColor="#FFFFFF"
                android:textSize="13sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/AddressOut"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="40dp"
                android:layout_marginBottom="35dp"
                android:gravity="left"
                android:letterSpacing="0.06"
                android:lineSpacingExtra="-6sp"
                android:text="Address"
                android:textColor="#FFFFFF"
                android:textSize="13sp"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintBottom_toTopOf="@+id/PhoneNumberOut"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/NameOut"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginStart="40dp"
                android:layout_marginBottom="35dp"
                android:gravity="left"
                android:letterSpacing="0.06"
                android:lineSpacingExtra="-6sp"
                android:text="Name"
                android:textColor="#FFFFFF"
                android:textSize="13sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/AddressOut"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/OutRentAddress"
                android:layout_width="132dp"
                android:layout_height="20dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="40dp"
                android:layout_marginBottom="35dp"
                android:gravity="end"
                android:letterSpacing="0.06"
                android:lineSpacingExtra="-6sp"
                android:text="Bekasi"
                android:textColor="#FFFFFF"
                android:textSize="13sp"
                app:layout_constraintBottom_toTopOf="@+id/OutRentPhoneNumber"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/OutRentPhoneNumber"
                android:layout_width="132dp"
                android:layout_height="18dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="40dp"
                android:layout_marginBottom="109dp"
                android:gravity="end"
                android:letterSpacing="0.06"
                android:lineSpacingExtra="-6sp"
                android:text="08123456789"
                android:textColor="#FFFFFF"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent" />

            <TextView
                android:id="@+id/OutRentName"
                android:layout_width="134dp"
                android:layout_height="15dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="40dp"
                android:layout_marginBottom="35dp"
                android:gravity="end"
                android:letterSpacing="0.06"
                android:lineSpacingExtra="-6sp"
                android:text="Alifya"
                android:textColor="#FFFFFF"
                android:textSize="13sp"
                app:layout_constraintBottom_toTopOf="@+id/OutRentAddress"
                app:layout_constraintEnd_toEndOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>



</androidx.constraintlayout.widget.ConstraintLayout>